[user]
  name = Kosuke Wakamatsu
  email = k.wakama@gmail.com
[color]
  # colorの設定(以下のコマンドは自動で色つける)
  status = auto
  diff = auto
  branch = auto
  interactive = auto
  grep = auto
[core]
  excludesfile = ~/.gitignore # globalな.gitignoreの指定
  #autocrlf = input # CRLFを自動変換
  editor = vim
  quotepath = false
[merge]
  tool = vimdiff
[alias]
	st = status
	br = branch
[http]
  sslVerify = false
